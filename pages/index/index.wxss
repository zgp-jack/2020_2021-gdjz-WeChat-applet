@import "../home/home.wxss";
@import "../../utils/phone.wxss";
/* lists-template-btn */
page{
    height: 100%;
}
.lists-header{
    position: fixed;
    width: 100%;
    background: #0099ff;
    height: 100rpx;
    left: 0;
    top: 0;
    padding-left: 27rpx;
    padding-right:110rpx;
    z-index: 15;
}
.lists-header input{
    height: 70rpx;
    line-height:70rpx;
    background: white;
    display: block;
    width: 100%;
    margin-top: 15rpx;
    border-radius:5px;
    padding-right:30rpx;
    padding-left: 30rpx;
    font-size: 28rpx;
}
.lists-header text{
    position: absolute;
    display: block;
    width: 80rpx;
    right:27rpx;
    top: 15rpx;
    line-height: 70rpx;
    text-align: right;
    color: white;
    font-size: 28rpx;
}
.lists-header image{
    position: absolute;
    display: block;
    width: 30rpx;
    height: 30rpx;
    left: 40rpx;
    top: 35rpx;
}

.lists-condition-menu{
    position: fixed;
    width: 100%;
    height: 80rpx;
    left: 0;
    top: 100rpx;
    background: white;
    z-index:9;
}
.lists-condition-item{
    position: relative;
    flex: 1;
    text-align:center;
    padding: 0 10rpx;
    height: 100%;
    overflow: hidden;
}
.lists-condition-item view{
    position: relative;
    display: inline-block;
    width: auto;
    max-width: 100%;
    text-align: center;
    font-size: 30rpx;
    height: 100%;
    line-height: 80rpx;
    color: #000;
    letter-spacing: 2rpx;
    padding-right: 30rpx;
    margin: 0 auto;
}
.lists-condition-item view image{
    position: absolute;
    width: 24rpx;
    height: 22rpx;
    right: 0;
    top: 30rpx;
    z-index: 1;
    transition: 0.5s;
}
.lists-condition-item.lists-condition-active view{
    color: #0099ff;
}
.lists-condition-item.lists-condition-active view image{
    transform: rotate(-180deg);
}
.user-choose-shade{
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.3);
    z-index: 5;
    opacity: 0;
    display: none;
    transition:opactiy 0.5s;
}
.user-choose-shade-active{
    opacity: 1;
    display: block;
}
.user-choose-item{
    position: absolute;
    width: 100%;
    height: 60%;
    background: white;
    left: 0;
    top: -70%;
    overflow-y: scroll;
    box-shadow: 0 1px 1px #e7e6e4;
    transition:top 0.5s;
}
.user-choose-item-active{
    top: 180rpx;
}
.filtrate-list-item{
    position: relative;
    width: 100%;
    height: 100rpx;
    padding: 0 27rpx;
    font-size: 30rpx;
    line-height: 100rpx;
}
.filtrate-list-active::before{
    position: absolute;
    content: '';
    width: 3px;
    height: 100%;
    background: #0099ff;
    left: 0;
    top: 0;
}
.filtrate-item-lists{
    position: fixed;
    right: -50%;
    width: 50%;
    height: 60%;
    top: 180rpx;
    background: #f7f7f7;
    z-index: 1;
    transition: 0.5s;
    overflow-y: scroll;
}
.filtrate-item-three2-lists{
    position: fixed;
    right: -66.66%;
    width: 66.66%;
    height: 60%;
    top: 180rpx;
    background: #f7f7f7;
    z-index: 1;
    transition: 0.5s;
    overflow-y: scroll;
}
.filtrate-item-three3-lists{
    position: fixed;
    right: -33.33%;
    width: 33.33%;
    height: 60%;
    top: 180rpx;
    background: #fff;
    z-index: 1;
    transition: 0.5s;
    overflow-y: scroll;
}
.filtrate-item-lists-active{
    right: 0;
}
.filtrate-item-info{
    color: #333;
}
.filtrate-item-info-active{
    background: #0099ff;
    color: #fff;
}
.yupao-lists-container{
    position: relative;
    padding-top: 180rpx;
    width: 100%;
    /* padding-left: 27rpx;
    padding-right: 27rpx;  */

    /* position: fixed;height:calc(100% - 280rpx);top: 180rpx;left: 0;padding-bottom: 0;overflow-y: scroll;box-sizing: border-box;  */
}

.yupao-lists-notice{
    position: relative;
    width: 100%;
    padding:15rpx 0 10rpx;
    margin-top: 15rpx;
    background: white;
    border-radius: 2px;
    box-shadow: #e7e6e4 0 0 10rpx;
}
.yupao-lists-wxinfo{
    position: relative;
    width: 100%;
    padding: 0 27rpx;
}
.yupao-common-tips{
    display: inline-block;
    width: initial;
    height: 40rpx;
    padding: 0 10rpx;
    background: #f86e09;
    border-radius: 2px;
    color: white;
    line-height: 40rpx;
    font-size: 22rpx;
}
.yupao-lists-wxtips{
    position: relative;
    font-size: 28rpx;
    line-height: 50rpx;
    color: #7f7e7e;
    border-bottom: 1px dashed #0099ff;
    padding-bottom: 10rpx;
}
.yupao-lists-wxtips text{
    color: #0099ff;
}
.yupao-lists-wxtips .yupao-lists-tips{
    position: relative;
    background: none;
    color: #f86e09;
}
.yupao-lists-noticetips{
    position: relative;
    width: 100%;
    height: 65rpx;
}
.yupao-lists-newtips{
    position: absolute;
    left: 27rpx;
    top: 15rpx;
    background: none;
    color: #f86e09;
    font-size: 28rpx;
    padding: 0;
    line-height: 40rpx;
}
.lists-notice-swiper{
    position: absolute;
    width: 100%;
    height: 50rpx;
    top:12rpx;
    padding-left: 160rpx;
}
.lists-notice-swiper swiper{
    height: 50rpx;
}
.lists-notice-text{
    position: relative;
    font-size: 28rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #0099ff;
    width:100%;
    display:block;
    padding-right: 27rpx;
}

.listsbox{
    position: relative;
    width: 100%;
    margin-top: 15rpx;
}
.listsbox .index-list-item{
    margin-bottom: 15rpx;
    box-shadow: #e7e6e4 0 0 10rpx;
    border-radius: 2px;
}
.lists-nodata{
    background: transparent;
}
.yupao-info-detailtitle{
    position: relative;
    font-size: 34rpx;
    line-height: 80rpx;
    border-bottom: 1px dashed #dcdcdc;
    margin-bottom: 10rpx;
}
.yupao-common-list-detail{
    border-bottom: 0;
    padding: 0 0 10rpx;
}
.yupao-common-list-detail .yupao-info-detailtitle , .yupao-common-list-detail .yupao-common-lists-text{
    padding: 0 27rpx;
}



/* 模板推送btn */
.lists-template-btn{position: absolute;width: 100%;height: 100%;display: block;background: transparent;z-index: 1;opacity: 0;left: 0;top: 0;}
.lists-template-btn::after,.lists-template-btn::before{border: 0;}


/* 一键发布 */
.bottompublishbtn{position: fixed;width: 300rpx;line-height: 80rpx;background: #0099ff;color: white;text-align: center;border-radius: 20px;bottom: 115rpx;left: 50%;margin-left: -150rpx;font-size: 28rpx;letter-spacing: 5rpx;box-shadow: #0099ff 0 0 10rpx;z-index: 3;}

.joblist-jz{position: absolute;left: 0;top: 0;width: 80rpx;height: 80rpx;z-index: 3;border-radius: 2px 0 0 0;}
.joblist-wyjz{position: absolute;bottom: 0;right: 0;width: 80rpx;height: 80rpx;z-index: 1;border-radius: 0 0 2px 0;}


.returntop{position: fixed;width: 80rpx;height: 80rpx;display: block;right: 50rpx;bottom: -120rpx;border-radius: 50%;box-shadow:0  0 5rpx #0099ff;transition: 0.5s;z-index: 10;}

.returntop-active{bottom: 115rpx;}


/* 反馈信息 */
.feedbackmsg{position: relative;width: 100%;padding:35rpx 20rpx;box-sizing: border-box;background: white;margin-top: 15rpx;padding-left: 60rpx;}
.feedbackmsg-img{position: absolute;width: 30rpx;height: 30rpx;left:20rpx;top:35rpx;}
.feedbackmsg view{font-size: 28rpx;color: #222121;line-height: 30rpx;padding-right: 155rpx;}
.feedbackmsg text{color: #ff0000;margin-right: 10rpx;}
.usersearchhistorybox{position: fixed;width: 100%;height: 100%;left: 0;top: 0;background: rgba(0, 0, 0, 0.3);z-index: 10;}
.usersearchhistory{position: absolute;width: 100%;top: 100rpx;left: 0;background: white;z-index: 15;}
.usersearchhistory-item{position: relative;width: 100%;padding: 20rpx;font-size:30rpx;color:#333;border-bottom: 1px solid #F0F2F5;line-height:60rpx;letter-spacing: 1rpx;}
.usersearchhistory-item-title{color:#777;line-height: 40rpx; }
.usersearchhistory-footer{position: relative;padding:50rpx 0;display: flex;flex-direction: row;justify-content: center;align-items: center;}
.usersearchhistory-footer view{position: relative;display: inline-block;line-height: 70rpx;padding: 0 50rpx;border: 1px solid #0099ff;border-radius: 5rpx;color: #0099ff;margin: 0 auto;font-size: 30rpx;}
.usersearchhistory-footer image{position: relative;display: inline-block;width: 32rpx;height: 32rpx;vertical-align: middle;margin-right: 10rpx;}
.feedback-rightarrow{position: absolute;right: 20rpx;width: 20rpx;height: 30rpx;display: block;top: 30rpx;}
.feedbackmsg .feedback-btn{position: absolute;right: 15rpx;color:white;line-height: 50rpx;padding: 0 20rpx;background: #0099ff;font-size: 24rpx;border-radius: 5rpx;top: 25rpx;}


/* 浮窗 2020-0203 */
.firstfcbox{position: fixed;left: 0;top: 0;z-index: 999;background: rgba(0, 0, 0, 0.3);width: 100%;height: 100%;}
.fctips-header{position: absolute;width:400rpx;height:100rpx;right: 120rpx;top:0;}
.fctips-body{position: absolute;width:100%;right: 0;bottom:0;}


