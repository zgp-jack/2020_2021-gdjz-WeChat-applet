    font-size: 28rpx;
    padding: 0;
    line-height: 40rpx;
}
.lists-notice-swiper{
    position: absolute;
    width: 100%;
    height: 50rpx;
    top:12rpx;
    padding-left: 160rpx;
}
.lists-notice-swiper swiper{
    height: 50rpx;
}
.lists-notice-text{
    position: relative;
    font-size: 28rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #0099ff;
    width:100%;
    display:block;
    padding-right: 27rpx;
}

.listsbox{
    position: relative;
    width: 100%;
    margin-top: 15rpx;
}
.listsbox .index-list-item{
    margin-bottom: 15rpx;
    box-shadow: #e7e6e4 0 0 10rpx;
    border-radius: 2px;
}
.lists-nodata{
