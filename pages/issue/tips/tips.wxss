page{
  background-color: white;
}
.issue-tip-container{
  position: relative;
  width: 100%;
  padding: 20rpx;
}
.success-box{
  position: relative;
  margin-top: 70rpx;
}
.success-tips-icon{
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
}
.success-box text{
  font-size: 28rpx;
  margin-top: 20rpx;
}
.success-tips-btns{
  position: fixed;
  padding: 0 50rpx;
  left: 0;
  width: 100%;
  bottom: 500rpx;
  justify-content: space-between;
  align-content: space-between;
}
.success-tips-btns button{
  background-color: #0099ff;
  color: #fff;
  font-size: 28rpx;
  width: 45%;
  text-align: center;
  line-height: 75rpx;
  border-radius: 5rpx;
}