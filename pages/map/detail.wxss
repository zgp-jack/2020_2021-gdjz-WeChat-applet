/* pages/map/detail.wxss */
page{background: white;}
.detail-info{
  position: relative;
  width: 100%;height: 100%;
  left: 0;
  top: 0;
  background: white;
  z-index: 100;
  overflow-y: scroll;
  transition: 0.5s;
}
.detail-info-active{
  left: 0;
}
.detail-info view{
  position: relative;
  font-size: 30rpx;
  padding: 20rpx 15rpx;
  border-bottom: 1px solid #dcdcdc;
  letter-spacing: 2rpx;
}
.detail-return{position: fixed;display: block;height: 80rpx;width: 80rpx;right: 20rpx;top: 80%;background: rgba(0, 0, 0, 0.1);border-radius: 50%;}