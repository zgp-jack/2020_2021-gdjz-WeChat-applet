@import "../course/course.wxss";
.published-title{
    position: relative;
    width: 100%;
    padding: 20rpx 27rpx;
    background: white;
    margin-top: 15rpx;
    font-size: 28rpx;
    color: #000;
}
.published-content{
    position: relative;
    width: 100%;
    padding: 20rpx 27rpx;
}

.published-list-item{
    position: relative;
}
.published-list-item{
    position: relative;
    width: 100%;
    background: white;
    margin-bottom: 15rpx;
}
.published-list-header{
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    padding-right: 150rpx;
    border-bottom: 1px dashed #0099ff;
}
.published-list-header text{
    position: absolute;
    color: red;
    display: block;
    line-height: 80rpx;
    right:27rpx;
    top: 10rpx;
}
.published-list-body{
    position: relative;
    height: 80rpx;
}
.published-list-body view{
    font-size: 28rpx;
    position: relative;
    width: 33.333%;
    float: left;
    text-align: left;
    line-height: 80rpx;
}
.published-list-body view image{
    position: relative;
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    margin-right: 10rpx;
    vertical-align: text-bottom;
}
.published-list-edit{
    position: relative;

}
.published-list-detail{
    position: absolute;
    left: 0;
    top: 0;
    text-align: center !important;
}
.published-list-settop{
    position: relative;
    float: right;
    text-align: right !important;
}
.published-list-footer{
    position: relative;
    border-top: 1px solid #dcdcdc;
    padding: 20rpx 0;
}
.published-list-footer image{
    position: relative;
    display: inline-block;
    width: 36rpx;
    height:36rpx;
    margin-right: 10rpx;
    vertical-align: text-bottom;
}
.published-list-footer view{
    font-size: 26rpx;
}
.cost-integral-settopbox{
    position: fixed;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    left: 0;
    top: 0;
    z-index: 10;
}
.cost-integral-settop{
    position: relative;
    width: 80%;
}
.cost-integral-settop-header{
    width: 100%;
    background: #f3f3f3;
    text-align: center;
    font-size: 34rpx;
    line-height:120rpx;
}
.cost-integral-settop-body{
    position: relative;
    width: 100%;
    height: 500rpx;
    background: white;
    padding: 40rpx 30rpx;
}
.cost-integral-settop-item{
    position: relative;
    width: 100%;
    height: 100rpx;
    margin-bottom: 20rpx;
    border: 1px solid #0099ff;
    padding-left: 160rpx;
}
.cost-integral-settop-tips{
    position: absolute;
    left: 20rpx;
    top: 0;
    line-height: 100rpx;
    font-weight: bold;
    font-size: 28rpx;
}
.cost-integral-settop-right{
    position: relative;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
}
.cost-integral-settop-footer{
    position: relative;
    width: 100%;
    height: 100rpx;
}
.cost-integral-settop-footer view{
    position: relative;
    display: block;
    width: 50%;
    height: 100rpx;
    float: left;
    padding: 0 20rpx;
}
.cost-integral-settop-footer view text{
    display: block;
    width: 100%;
    height: 80%;
    background: rgba(0,0,0,.1);
    color: #353535;
    text-align: center;
    line-height: 80rpx;
    font-size: 28rpx;
}
.cost-integral-settop-footer view:nth-last-child(1) text{
    background: #0099ff;
    color: #fff;
}
.cost-integral-settop-num{
    position: relative;
    line-height: 80rpx;
    font-size: 34rpx;
    display: block;
    text-align: center;
    margin-bottom: 20rpx;
    color: #0099ff;
}
.cost-integral-settop-num text{
    color: #ff0000;
    margin: 0 5rpx;
}



/* 新版本置顶 */
.user-province-settop{position: fixed;width: 100%;height: 100%;left: 0;top: 0;z-index: 50;background: rgba(0, 0, 0, 0.3);}
.cost-integral-settop{width: 90%;}
.cost-integral-settop-header{line-height: 100rpx;}
.psettop-body{position: relative;width: 100%;padding: 30rpx 20rpx;background: white;}
.settop-checkbox{position: relative;display: inline-block;font-size: 28rpx;width: 33.333%;margin-bottom: 10rpx;}
.settop-checkbox checkbox .wx-checkbox-input {width: 40rpx;height: 40rpx;}











