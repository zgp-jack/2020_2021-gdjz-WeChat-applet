@import "../../userauth/userauth.wxss";
@import "../../home/home.wxss";
/* publish-choose-wordtype common-publish-words*/
.common-publish-header{
    position: relative;
    width: 100%;
    height: 190rpx;
    background:url("http://cdn.yupao.com/miniprogram/images/publishbg.png") no-repeat;
    background-size:100% 100%;
}
.common-publish-title{
    position: relative;
    width: 100%;
    display: block;
    color: #fff;
    line-height: 130rpx;
    font-size: 38rpx;
    letter-spacing: 2px;
    text-align: center
}
.common-publish-tipsbox{
    position: absolute;
    width: 100%;
    left: 0;
    bottom: -60rpx;
    padding: 0 27rpx;
}
.common-publish-tips{
    position: relative;
    width: 100%;
    background: white;
    height: 120rpx;
    line-height: 120rpx;
    border-radius: 5px;
    box-shadow: #e7e6e4 0 0 10rpx;
    text-align: center;
    font-size: 32rpx;
    letter-spacing: 1px;
}
.common-publish-body{
    position: relative;
    width: 100%;
    padding: 75rpx 27rpx 0;
}
.recruit-publish-body{padding-top: 20rpx;}
.common-publish-content{
    position: relative;
    width: 100%;
    padding: 20rpx 0 0;
    background: white;
}
.common-publish-item{
    position: relative;
    padding:0 20rpx;
    min-height: 100rpx;
}
.common-publish-item-content{
    padding: 20rpx 0;
    padding-left: 180rpx;
    border-bottom: 1px solid #dcdcdc;
    overflow: hidden;
}
.common-publish-content .common-publish-item:nth-last-child(1) .common-publish-item-content{
    border-bottom: 0;
}
.common-publish-words{
    position: absolute;
    left: 20rpx;
    top: 20rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    font-weight: bold;
    width: 140rpx;
    text-align: justify;
}
.common-publish-words::after{
    content:'.';
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 0;
}
.common-publish-input{
    position: relative;
    width: 100%;
    display: block;
    height: 60rpx;
    font-size: 30rpx;
    line-height: 20rpx;
    border: 0;
}
.common-publish-allwords{
    position: relative;
    display: block;
    padding:10rpx 0;
    line-height: 40rpx;
    font-size: 30rpx;
}
.allow-not-uploads{
    position: relative;
    padding-left: 50rpx;
}
.common-uploads-bgbox{
    position: absolute;
    left: 0;
    top: 20rpx;
    display: block;
    width:40rpx;
    height: 40rpx;
    background: url("http://cdn.yupao.com/miniprogram/images/upload-bg.png") no-repeat;
    background-size:100% 100%;
}
.common-uploads-bgbox text{
    position: absolute;
    display: block;
    width: 16rpx;
    height: 16rpx;
    bottom: 10rpx;
    left: 12rpx;
    background: url("http://cdn.yupao.com/miniprogram/images/upload-down.png") no-repeat;
    background-size: 100% 100%;
    transform-origin:center;
    transition: 0.5s;
}
.user-uploads-active{
    transform:rotateX(-180deg);
}
.common-publish-item-code{
    padding-right: 220rpx;
}
.publish-getcode-btn{
    position: absolute;
    right:27rpx;
    background: #0099ff;
    color: white;
    font-size: 30rpx;
    top: 20rpx;
    display: block;
    width: 200rpx;
    height:60rpx;
    text-align: center;
    border-radius: 2px;
    line-height: 60rpx;
}
.common-publihs-contenttips{
    position: relative;
    display: block;
    font-size: 30rpx;
    font-weight: bold;
    padding:20rpx 0;
    height: 60rpx;
    width: 140rpx;
    text-align: justify;
}
.common-publihs-contenttips::after{
    content:'.';
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 0;
}
.common-publish-item textarea{
    position: relative;
    margin-top: 20rpx;
    display: block;
    width: 100%;
    height: 200rpx;
    font-size: 30rpx;
    line-height:40rpx;
    z-index: 0;
    padding-bottom: 60rpx;
}
.publish-uploads-img{
    margin-top: 15rpx;
}
.publish-uploads-title{
    text-align: center;
    font-size: 34rpx;
    line-height: 80rpx;
    letter-spacing: 1px;
}
.publish-uploads-title view{
    color: #ff0000;
    display: inline-block;
    font-size: 30rpx;
}
.publish-uploads-content{
    position: relative;
    width: 100%;
    padding:0 10rpx;
}
.publish-uploads-item{
    position: relative;
    display: block;
    width: 33.3333%;
    float: left;
    height: 180rpx;
    padding: 10rpx;
}
.uploads-item-content{
    position: relative;
    display: block;
    width: 100%;
    height: 100%;
    border: 1px solid #dcdcdc;
    border-radius: 5px;
    overflow: hidden;
}
.uploads-item-content image{
    position: relative;
    display: block;
    width: 100%;
    height: 100%;
}

.publish-choose-wordtype{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.6);
    z-index:100;
}
.publish-choose-contentbox{
    position: absolute;
    width: 80%;
    height: 80%;
    background: white;
    left: 10%;
    top: 10%;
    padding: 0;
}
.publish-choose-header{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100rpx;
    background: #f3f3f3;
    text-align: center;
    line-height: 100rpx;
    font-size: 30rpx;
    letter-spacing: 2rpx;
    font-weight: 600;
    z-index: 2;
}
.publish-choose-footer{
    position: absolute;
    width: 100%;
    bottom: 0;
    height: 120rpx;
    line-height: 120rpx;
    border-top: 1px solid #dcdcdc;
    z-index: 2;
    background: white;
}
.publish-choose-footer view{
    flex: 1;
    text-align: center;
    font-size: 30rpx;
    height:inherit;
    line-height: inherit;
    letter-spacing: 2rpx;
    border-right: 1px solid #dcdcdc;
}
.publish-choose-footer view:nth-last-child(1){
    border-right: 0;
}
.publish-choose-body{
    position: relative;
    width: 100%;
    height: 100%;
    padding-top: 100rpx;
    padding-bottom: 120rpx;
    z-index: 1;
    overflow-y: scroll;
    background: white;
}
.publish-choose-content-item{
    position: relative;
    padding: 10rpx 20rpx;
}
.publish-choose-content-header{
    position: relative;
    width: 100%;
    padding: 10rpx 0;
    font-size: 34rpx;
    line-height: 60rpx;
    font-weight: bold;
}
.publish-choose-content-list{
    position: relative;
    width: 100%;
    padding: 20rpx 0;
    line-height: 40rpx;
}
.choose-content-lish-left{
    position: relative;
    padding-right: 100rpx;
    width: 100%;
    font-size: 34rpx;
    display: block;
}
.choose-content-checkbox{
    position: absolute;
    width: 46rpx;
    height: 46rpx;
    border-radius: 50%;
    border: 1px solid #dcdcdc;
    display: block;
    top: 18rpx;
    right: 0;
    z-index: 2;
}
.choose-content-checkbox-active{
    background: #0099ff;
}


.common-publish-item-pass{
    padding-right: 80rpx;
}
.pass-eyes-icon{
    position: absolute;
    display: block;
    width: 39rpx;
    height:23rpx;
    right: 30rpx;
    top: 40rpx;
    background: url("http://cdn.yupao.com/miniprogram/images/eyes.png") no-repeat;
    background-size: 100% 100%;
    filter:grayscale(100%);
    z-index: 1;
}
.pass-eyes-icon-active{
    filter:grayscale(0);
}
.user-items-text{
    margin-right: 20rpx;
}
.common-publish-allwords .user-items-text:nth-last-child(1){
    margin-right: 0;
}
.publish-choose-body-padding{
    padding: 120rpx 20rpx 120rpx;
}

.del-upload-item-img{
    position: absolute;
    width: 100%;
    height: 60rpx;
    left: 0;
    bottom: 0;
    z-index: 5;
    background: rgba(0, 0, 0, 0.4);
}
.del-upload-item-img image{
    position: relative;
    display: block;
    width: 40rpx;
    height: 40rpx;
    margin: 10rpx auto;
}

