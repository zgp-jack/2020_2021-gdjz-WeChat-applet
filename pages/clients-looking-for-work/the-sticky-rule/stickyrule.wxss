@import "../../home/home.wxss";

.index-area-body{
  padding-top: 0rpx;
  position: static;
  padding-bottom: 120rpx;
}
.index-area-tips{
  position: absolute;
  top: 0rpx;
}
.area-header-words{
  padding-left: 10rpx;
  font-weight: bold;
}
.obtn{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-color: white;
}
.obtn button{
  width: 500rpx;
  height: 95rpx;
  background: #108EEF;
  line-height: 100rpx;
  text-align: center;
  color: white;
  font-size: 45rpx;
  letter-spacing: 4rpx;
}