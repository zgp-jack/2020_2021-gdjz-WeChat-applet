/* 遮罩层 */
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.5;
}
  
/* 弹出层 */
.modalDlg{
    width: 70%;
    position: fixed;
    top: 340rpx;
    left: 0;
    right: 0;
    z-index: 9999;
    margin: 0 auto;
    background-color: #fff;
    border-radius:5px;
    display: flex;
    flex-direction: column;
    align-items: center;
    border: 1px solid rgb(143, 142, 142);
}
.modalDlg .modalDlgone{
  width: 40%;
  margin: 6rpx auto;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
}

.text{
    width: 100%;
    margin:0 auto;
    height: 280rpx;
    font-size: 32rpx;
    position: relative;
}

.text .otext{
  width: 85rpx;
  font-size: 42rpx;
  font-weight: bold;
  margin: 0 auto;
}
.text .textdetail{
  font-size: 32rpx;
  width: 80%;
  margin: 28rpx auto;
}
.buttontext {
   width: 100%;
   margin:0 auto;
   display: flex;
   justify-content: space-between;
   position: absolute;
   bottom: 0;
}
.buttontext  view{
   width: 50%;
   height: 80rpx;
   background:#0099FF;
   font-size: 32rpx;
   text-align: center;
   line-height: 80rpx;
}

.buttontext  view:first-child{
   border: 1px solid rgb(143, 142, 142);
   color: black;
   background: white;
   border-left: 0rpx;
   border-bottom: 0rpx;
}

.buttontext  view:last-child{
   color: white;
}

.delete{
   width: 30rpx;
   height: 30rpx;
   background: rgb(139, 137, 137);
   position: absolute;
   border-radius: 50%;
   right: 0;
   top: 0;
   text-align: center;
   line-height:  30rpx;
   color: white;
   font-size: 20rpx;
}
