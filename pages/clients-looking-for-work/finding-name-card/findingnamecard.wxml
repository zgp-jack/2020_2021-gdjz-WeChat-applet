  <view class="cardcolore">
    <view class="findingnamecardsix" wx:if="{{projectlength == 0}}">
      <text class="findingnamecardsixone">添加项目经验，可提升老板对您的信任程度</text>
      <view class="findingnamecardsixtwo" bindtap="addproject">添加项目经验</view>
