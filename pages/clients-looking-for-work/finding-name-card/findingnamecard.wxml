            <view>
              <image src="{{downward}}" class="down"></image>
            </view>
          </view>
          <view wx:if="{{checkthreef == 0}}" class="more">
            修改项目经验
            <view>
              <image src="{{downward}}" class="down"></image>
            </view>
          </view>
          <view wx:if="{{checkthreef == 0}}" class="cardsixsixnum">
