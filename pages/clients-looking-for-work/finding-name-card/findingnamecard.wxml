          </view>
          <view class="certificatetime">
            <text>{{item.certificate_time}}</text>
          </view>
          <view class="cardeightfive">
            <image wx:for="{{item.image}}" data-url="{{ item }}" catchtap="previewImagec" src="{{item}}" data-index="{{ pindex }}" wx:key="{{index}}"></image>
