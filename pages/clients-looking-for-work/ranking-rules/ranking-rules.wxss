}
.ranking-childbote{
  width: 94%;
  margin: auto;

}
.ranking-num{
  display: inline-block;
  width: 38rpx;
  height: 38rpx;
  background: #0099ff;
  text-align: center;
  line-height: 38rpx;
  color: white;
  font-size: 26rpx;
  border-radius:50%; 
  vertical-align: middle;

}

.ranking-zan{
  font-size: 30rpx;
}
.ranking-point{
    font-size: 28rpx;
    color: #999999;
