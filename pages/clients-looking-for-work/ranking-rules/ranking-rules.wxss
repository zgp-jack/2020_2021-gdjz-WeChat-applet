.header {
  margin: 0 auto;
  background: white;
  /* color: #f87a14; */
  /* text-align: center; */
  /* line-height: 80rpx; */
  position: fixed;
  width: 100%;
  top: 0;
  padding: 20rpx 32rpx;
  font-size: 0;
  border-bottom: 1rpx solid rgb(233, 232, 232);
  z-index: 2;
}
.header .headerpoint{
  padding-bottom: 10rpx;
}
.header .headerpoint text{
    font-size: 32rpx;
}
.header .headerpoint text:last-child{
   color: #0099ff;
   font-weight: bold;
}
.headerdetail{
  color: #f87a14;
  font-size: 0rpx;
}
.headerdetail text{
    font-size: 30rpx;
}
.ranking-center{
  background: white;
  margin-top: 188rpx;
  overflow: hidden;
}
.ranking-apple{
  height: 120rpx;
}
.ranking-child{
  width: 94%;
  border-bottom: 1rpx dashed rgb(209, 209, 209);
  padding: 26rpx 0;
    margin: auto;
}
.ranking-child:last-child{
    width: 94%;
    margin: auto;
   border-bottom: 0;
     display: flex;
}
.ranking-childone{
  display: flex;
  align-items: center;
  padding-right: 150rpx;
  position: relative;
}
.ranking-obtn{
  width: 21%;
  background: #0099ff;
  color: white;
  font-size: 30rpx;
  padding: 12rpx 0;
  line-height: 40rpx;
  border-radius: 12rpx;
  text-align: center;
  position: absolute;
  right: 0;
  margin:0rpx;
}

.ranking-obtn::after {
  border: none;
}
.ranking-oimg{
  font-size: 0;
}
.ranking-oimg image{
  width: 80rpx;
  height: 80rpx;
}
.ranking-detail{
  /* display: flex;
