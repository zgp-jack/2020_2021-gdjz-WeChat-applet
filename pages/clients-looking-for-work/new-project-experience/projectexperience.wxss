}
  
/* 弹出层 */
.modalDlg{
    width: 70%;
    position: fixed;
    top: 340rpx;
    left: 0;
    right: 0;
    z-index: 9999;
    margin: 0 auto;
    background-color: #fff;
    border-radius:5px;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.modalDlg .modalDlgone{
  width: 40%;
  margin: 6rpx auto;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
}
.bg {
  position: fixed;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 8888;
  -moz-opacity: 0.7;
  opacity: 0.70;
  filter: alpha(opacity=70);
}
