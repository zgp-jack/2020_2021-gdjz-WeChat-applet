page{
    height: 100%;
    width: 100%;
    position: relative;
}
.help_center_head{
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: flex-end;
}
.help_center_search{
    width: 90%;
    height: 70rpx;
    border-radius: 5rpx;
    display: flex;
    align-items: center;
    background-color: #ffff;
    box-shadow: 1rpx 2rpx 7rpx 0rpx rgba(177, 177, 177, 0.31);
}
.help_search_icon{
    padding: 5rpx 20rpx;
    width: 35rpx;
    height: 35rpx;
}
.heip_search_input{
    flex: 1;
    height: 100%;
    padding-left: 10rpx;
    font-size: 28rpx;
}

.help_search_list{
    width: 100%;
    height: 200rpx;
    display: flex;
    justify-content: center;
    position: absolute;
    margin-top: 10rpx;
    z-index: 100;
}
.help_search_content{
    width: 90%;
    border-radius: 5rpx;
    background-color: #ffff;
    box-shadow: 1rpx 2rpx 7rpx 0rpx rgba(177, 177, 177, 0.31);
}


.help_list_scroll{
    height: calc(100vh - 420rpx);
    width: 100%;
}
.help_list_title{
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.help_list_title view{
    width: 90%;
    height: 70rpx;
    display: flex;
    align-items: center;
    font-size: 35rpx;
    font-weight: 500;
    color: #303030;
}
.help_list_body{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.help_list_item{
    width: 90%;
    display: flex;
    flex-direction: column;
    align-items: center;
    border: 1rxp solid black;
    margin-bottom: 10rpx;
}
.help_list_question{
    display: flex;
    align-items: center;
    border-bottom: 1px solid #EEEEEE;
    height: 90rpx;
    width: 100%;
}
.help_question_icon{
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    border-radius: 4rpx;
}
.help_question_icon image{
    width: 100%;
    height:100%;
}
.help_question_content{
    margin-right: auto;
    font-size: 30rpx;
    font-weight: 400;
    color: #363636;
}
.help_question_arrow{
    height: 25rpx;
    width: 15rpx;
}
.icon_down_rotate{
    transform: rotate(90deg);
}

.help_question_detail{
    width: 100%;
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-bottom: 10rpx;
    font-size: 28rpx;
}
.help_question_item {
    display: flex;
    align-items: center;
    height: 90rpx;
    width: 100%;
}
.help_center_footer{
    height: 200rpx;
    width: 100%;
    position: absolute;
    bottom: 0rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;

}
.help_center_suggestions{
    width: 80%;
    background-color: #0099FF;
    border-radius: 5rpx;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #ffff;
}
.help_my_suggestions{
    width: 80%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #0099FF;
}

