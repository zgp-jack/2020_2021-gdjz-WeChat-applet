.realname-query-box{
    position: relative;
    width: 100%;
    padding:10rpx 27rpx 0;
}
.realname-query-header{
    position: relative;
    padding: 30rpx 50rpx;
    background: white;
    border-radius: 2px;
    box-shadow: #e7e6e4 0 0 10rpx;
    margin-bottom: 15px;
}
.realname-query-header input{
    border-bottom: 1px solid #dcdcdc;
    text-align: center;
    font-size: 30rpx;
    display: block;
    height: 80rpx;
    width: 100%;
    line-height:80rpx;
}
.realname-query-header text{
    position: relative;
    display: inline-block;
    font-size: 28rpx;
    background: #0099ff;
    height: 70rpx;
    line-height: 70rpx;
    padding: 0 50rpx;
    color: white;
    margin-top:20rpx;
    border-radius: 2px;
}
.realname-query-body{
    position: relative;
    padding-bottom: 15rpx;
    border-radius: 2px;
}
.realname-query-body .yupao-common-list-left{
    width: auto;
}
.realname-query-footer{
    position: relative;
    width: 100%;
    padding: 20rpx 27rpx;
    background: white;
    box-shadow: #e7e6e4 0 0 10rpx;
    border-radius: 2px;
}
.realname-query-tips{
    position: relative;
    background: #f86e09;
    color: white;
    display: inline-block;
    height: 50rpx;
    line-height: 50rpx;
    padding:0 20rpx;
    font-size: 26rpx;
    border-radius: 2px;
}
.realname-query-words{
    position: relative;
    width: 100%;
    padding-top: 15rpx;
}
.realname-query-words view{
    font-size: 30rpx;
    line-height:56rpx;
}
.realname-query-words view text{
    color: #0099ff;
}