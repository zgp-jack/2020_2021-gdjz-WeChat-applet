page {
    position: relative;
    width: 100%;
    height: 100%;
    background: #f7f6f9;
}

.common-userauth {
    position: fixed;
    left: 0;
    top: 0;
    z-index: 999999;
    width: 100%;
    height: 100%;
    background: white;
}

.feedback-header {
    position: relative;
    width: 100%;
    background: #fff;
    padding: 30rpx;
