.issueok-container{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.3);
  left: 0;
  top: 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  z-index: 99999;
}
.issueok-box{
  position: relative;
  width: 80%;
  background-color: white;
  border-radius: 10rpx;
  overflow: initial;
}
.issueok-title{
  position: relative;
  line-height: 90rpx;
  font-size: 40rpx;
  text-align: center;
  padding-top: 25rpx;
  font-weight: bold;
}
.issueok-body{
  position: relative;
  line-height: 65rpx;
  font-size: 35rpx;
  text-align: center;
  padding: 0rpx 55rpx;
}
.issueok-footer{
  position: relative;
  border-top: 1rpx solid #eeeeee;
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  overflow: hidden;
}
.issueok-btn{
  font-size: 35rpx;
  flex: 1;
  border-right: 1rpx solid #eeeeee;
  background: white;
  border-radius: 0;
  line-height: 100rpx;
  font-weight: bold;
}
.issueok-btn::after{
  border: 0;
}
.issueok-btn:nth-last-child(1){
  border-right: 0;
  color: #0099ff;
}
.issueok-close{
  position: absolute;
  bottom: -100rpx;
  width: 50rpx;
  height: 50rpx;
  left: 50%;
  margin-left: -25rpx;
  border-radius: 50%;
}